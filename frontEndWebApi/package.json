{
  "name": "angular-ucb-kickstart",
  "version": "1.1.1",
  "fullversion": "1.1.1-alpha",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "test": "npm run lint && ng test --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --browsers ChromeHeadless --watch",
    "build:prod": "ng build --prod --build-optimizer --vendor-chunk --common-chunk",
    "clean": "rimraf ./dist/",
    "server": "cd dist && http-server",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && ng lint && ng test --code-coverage=true --watch=false && npm run build:prod",
    "release": "standard-version && git push --follow-tags origin master",
    "prettier": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "prettier:ci": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@auth0/angular-jwt": "^2.1.0",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "adal-angular": "=1.0.17",
    "applicationinsights-js": "^1.0.20",
    "bootstrap": "^4.3.1",
    "browser-detect": "^0.2.28",
    "core-js": "^2.6.5",
    "fast-json-patch": "^2.0.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-markdown": "^8.1.0",
    "rxjs": "^6.5.2",
    "tslint-sonarts": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "^8.0.6",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@types/adal-angular": "1.0.1",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^10.12.26",
    "@types/applicationinsights-js": "^1.0.9",
    "codelyzer": "^5.0.1",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-sonarqube-unit-reporter": "^0.0.18",
    "karma-spec-reporter": "^0.0.32",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.16.4",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.3",
    "sonar-scanner": "^3.1.0",
    "standard-version": "^5.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "ajv": "^6.9.2"
  }
}
